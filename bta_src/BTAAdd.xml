<dialog title="Generate A Texture Atlas" buttons="accept">

<label value="Additional Params"/>

<radiogroup id="params" groupbox="true" >

<grid>
    <columns>
        <column/>
        <column/>
        <column/>
    </columns>
    <rows>
        <row>
            <checkbox label="Inline Symbols" id="InSym"/>
            <checkbox label="Bake All Texts" id="BATX"/>
        </row>
        <row>
            <checkbox label="Include Sounds" checked="true" id="INCS"/>
        </row>
        <row>
            <checkbox label="Bake One Frame Symbols" checked="true" id="BOFS"/>
        </row>
    </rows>
</grid>

</radiogroup>

</dialog>